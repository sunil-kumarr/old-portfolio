<script>
  // The view visiblility checker
  const isViewInViewport = el => {
  	const scroll = window.scrollY || window.pageYOffset
  	const boundsTop = el.getBoundingClientRect().top + scroll
  
  	const viewport = {
  		top: scroll,
  		bottom: scroll + window.innerHeight,
  	}
  
      const bounds = {
  		top: boundsTop,
  		bottom: boundsTop + el.clientHeight,
  	}
  
  	return ( bounds.bottom >= viewport.top && bounds.bottom <= viewport.bottom ) 
  		|| ( bounds.top <= viewport.bottom && bounds.top >= viewport.top );
  }
  
  
  // Usage.
  document.addEventListener( 'DOMContentLoaded', () => {
  	const tester = document.querySelector( '#nav-section' )
  	const answer = document.querySelector( '.side-crawler-class' )
  	
  	const handler = () => raf( () => {
  		answer.style.display = ( isViewInViewport( tester ) ? 'none' : 'block' )
  	} )
  	
  	handler()
  	window.addEventListener( 'scroll', handler )
  } )
  
  // requestAnimationFrame
  const raf = 
      window.requestAnimationFrame ||
      window.webkitRequestAnimationFrame ||
      window.mozRequestAnimationFrame ||
      function( callback ) {
          window.setTimeout( callback, 1000 / 60 )
      }
</script>